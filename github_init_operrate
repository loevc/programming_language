# create a new repository on the command line
echo "# programming_language" >> README.md
git init
git add README.md
git commit -m "first commit"
git branch -M main
git remote add origin git@github.com:loevc/programming_language.git
git push -u origin main

# push an existing repository from the command line
git remote add origin git@github.com:loevc/programming_language.git
git branch -M main
git push -u origin main
